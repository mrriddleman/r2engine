namespace flat;

file_identifier "rlpk";
file_extension "rlpk";

table LevelRef
{
	levelName:string;
	levelPath:string;
}

table LevelGroup
{
	groupName:string;
	gourpHash:ulong(hash:"fnv1a_64");
	maxLevelsInAGroup:uint;
	maxLevelSizeInGroup:uint;
	levels:[LevelRef];
}

table LevelPack
{
	maxLevelSize:uint;
	allLevels:[LevelGroup];
}

root_type LevelPack;